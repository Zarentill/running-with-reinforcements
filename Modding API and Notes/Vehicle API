******************************************Character in Vehicle

character_slot:
		type=
			"driver"
				position="cords"
				rotation="int"
				exit_rotation="int"
				hiding="bool"
				animation_id=
				animation_key=
						"driving car, headbang"
			"gunner"
				attached_on_turret="bool"
				seat_position="cords"
				enter_position="cords"
				rotation="int"
				exit_rotation="int"
				hiding="bool"
				should_fight_vehicles_only="bool"
				animation_id=
				animation_key=
						"tank mg still"									
					<state class="entering" animation_key="crouching forwards short, no weapon" position="0.2 0 -0.7" rotation="0" />
					<state class="leaving" animation_key="crouching forwards short, no weapon" position="0.2 0 -0.7" rotation="3.14" />
					<state class="idle" animation_key="crouch still, no weapon, 62" position="0.0 2.0 -0.0" />
					##<state class="attached_on_turret" />
					<turret index="0" />
					<turret index="1" />
			"passenger"
				position="cords"
				rotation="int"
				exit_rotation="int"
				hiding="bool"
				animation_id=
				allow_weapon="bool"
				animation_key=
						"travelling car, aiming, left"
						"driving car, headbang"

