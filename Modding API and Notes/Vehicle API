******************************************Vehicle Operational Statistics

control:
	max_speed="int"

	acceleration="int"

	max_reverse_speed="int"

	max_rotation="int"

	max_water_depth="int"

	can_steer_in_water="bool"

	can_steer_on_ground="bool"

	steer_smoothening="int"

	tracked="bool"

	gears=??

******************************************Vehicle Physical Statistics

physics:
	max_health="int"

	mass="int"

	extent="cords"			{expands the baseline of the model affected by gravity, and the pull from the tire joints, along the X and Z axis}

	offset="cords"			{moves the base line of the model affected by gravity, and the pull from the tire joints, in relation to the XYZ position}

	top_offset="cords"		{moves the position of the pyramid point that extends up from the primary offest. Functions as a center of gravity}

	collision_model_pos="cords"	{moves the collision detection box of the model in relation to the XYZ axis}

	collision_model_extent="cords"	{expands or contracts the collision detection box of the model along the XYZ axis}

	visual_offset="cords"		{moves the rendered model of the object in relation to the XYZ axis}

	preserve_motion_on_impact_factor=

	collision_response_factor=
	
	gravity="int"			{forwatever reason, negative on the Y axis seems to be down gravity and postive is up}
	
	gravity_in_water="int"		{forwatever reason, negative on the Y axis seems to be down gravity and postive is up}
	
	##collision_softness=???
	
	##aabb=???

