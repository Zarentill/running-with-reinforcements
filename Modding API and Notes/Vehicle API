******************************************Primary File Attributes************************************************************************************************************************************************
Vehicle:
	file="string"

	name="string"

	key="string"

	map_view_atlas_index="string"

	jams_enemy_radio="bool"

	minimum_fill_requirement="int"
	
	needs_protection="bool"

	designated_squad_as_driver_only="bool"

	allow_character_leave_request="bool"

	allow_owner_to_use_only="bool"

	allow_ai_to_use="bool"

	respawn_time="int"

	existence=
		"one_per_faction"
		"one_per_spawn_point"

	reserves_spawn_point=???

	spottable="bool"

	protectors="int"

	soldier_capacity_offset="int"

	should_be_destroyed="bool"
	
	time_to_live=???

